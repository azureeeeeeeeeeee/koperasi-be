                    "title": {
                        "type": "string",
                        "example": "Produk A"
                    },
                    "description": {
                        "type": "string",
                        "example": "Deskripsi produk A"
                    },
                    "price": {
                        "type": "number",
                        "format": "float",
                        "example": 10000
                    },
                    "stock": {
                        "type": "integer",
                        "example": 15
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2024-01-01T00:00:00Z"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2024-01-01T00:00:00Z"
                    }
                },
                "type": "object"
