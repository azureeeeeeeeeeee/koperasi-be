            "put": {
                "tags": [
                    "Admin"
                ],
                "summary": "Update User Data",
                "description": "Update user information",
                "operationId": "9993545d1210500aa0e319caf69fea80",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "User ID",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "fullname": {
                                        "type": "string",
                                        "example": "John Doe Updated"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "updated@student.itk.ac.id"
                                    },
                                    "tipe": {
                                        "type": "string",
                                        "enum": [
                                            "pengguna",
                                            "pegawai",
                                            "penitip",
                                            "admin"
                                        ],
                                        "example": "pengguna"
                                    },
                                    "status_keanggotaan": {
                                        "type": "string",
                                        "enum": [
                                            "aktif",
                                            "tidak aktif",
                                            "suspended"
                                        ],
                                        "example": "active"
                                    },
                                    "saldo": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 50000
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "newpassword123"
                                    },
                                    "password_confirmation": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "newpassword123"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "User berhasil diupdate",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Data user berhasil diperbarui"
                                        },
                                        "data": {
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "User tidak ditemukan"
                    },
                    "422": {
                        "description": "Validasi error"
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            },
            "delete": {
                "tags": [
                    "Admin"
                ],
                "summary": "Delete User",
