                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "properties": {
                                                "id": {
                                                    "type": "integer",
                                                    "example": 1
                                                },
                                                "fullname": {
                                                    "type": "string",
                                                    "example": "John Doe"
                                                },
                                                "email": {
                                                    "type": "string",
                                                    "example": "john@student.itk.ac.id"
                                                },
                                                "tipe": {
                                                    "type": "string",
                                                    "example": "pengguna"
                                                },
                                                "status_keanggotaan": {
                                                    "type": "string",
                                                    "example": "aktif"
                                                },
                                                "created_at": {
